<apex:page showHeader="false" id="Hos_RandomDoctorsPublicSite" controller="Hos_RandomizeDoctorsHandler">
    <apex:pageBlock title="{!$Label.Random_Doctors}">
        <apex:pageBlockTable value="{!randomDoctors}" var="d">
            <apex:column value="{!d.Full_Name__c}"/>
            <apex:column value="{!d.Email__c}"/>
            <apex:column value="{!d.Country__c}"/>
        </apex:pageBlockTable>
    </apex:pageBlock>
</apex:page>